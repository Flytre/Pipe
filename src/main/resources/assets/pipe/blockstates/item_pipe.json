{
  "multipart": [
    {
      "apply": {
        "model": "pipe:block/item_pipe/node"
      }
    },
    {
      "when": {
        "pipe_north": "connected"
      },
      "apply": {
        "model": "pipe:block/item_pipe/connector",
        "x": 270
      }
    },
    {
      "when": {
        "pipe_east": "connected"
      },
      "apply": {
        "model": "pipe:block/item_pipe/connector",
        "x": 270,
        "y": 90
      }
    },
    {
      "when": {
        "pipe_south": "connected"
      },
      "apply": {
        "model": "pipe:block/item_pipe/connector",
        "x": 270,
        "y": 180
      }
    },
    {
      "when": {
        "pipe_west": "connected"
      },
      "apply": {
        "model": "pipe:block/item_pipe/connector",
        "x": 270,
        "y": 270
      }
    },
    {
      "when": {
        "pipe_up": "connected"
      },
      "apply": {
        "model": "pipe:block/item_pipe/connector",
        "x": 180
      }
    },
    {
      "when": {
        "pipe_down": "connected"
      },
      "apply": {
        "model": "pipe:block/item_pipe/connector"
      }
    },

    {
      "when": {
        "pipe_north": "servo"
      },
      "apply": {
        "model": "pipe:block/item_pipe/servo",
        "x": 270
      }
    },
    {
      "when": {
        "pipe_east": "servo"
      },
      "apply": {
        "model": "pipe:block/item_pipe/servo",
        "x": 270,
        "y": 90
      }
    },
    {
      "when": {
        "pipe_south": "servo"
      },
      "apply": {
        "model": "pipe:block/item_pipe/servo",
        "x": 270,
        "y": 180
      }
    },
    {
      "when": {
        "pipe_west": "servo"
      },
      "apply": {
        "model": "pipe:block/item_pipe/servo",
        "x": 270,
        "y": 270
      }
    },
    {
      "when": {
        "pipe_up": "servo"
      },
      "apply": {
        "model": "pipe:block/item_pipe/servo",
        "x": 180
      }
    },
    {
      "when": {
        "pipe_down": "servo"
      },
      "apply": {
        "model": "pipe:block/item_pipe/servo"
      }
    }
  ]
}