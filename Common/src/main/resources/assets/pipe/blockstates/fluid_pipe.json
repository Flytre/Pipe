        "pipe_up": "none",
        "pipe_south": "none"
      },
      "apply": {
        "model": "pipe:block/fluid_pipe/side_top",
        "y": 180
      }
    },
    {
      "when": {
        "pipe_up": "none",
        "pipe_west": "none"
      },
      "apply": {
        "model": "pipe:block/fluid_pipe/side_top",
        "y": 270
      }
    },
    {
      "when": {
        "pipe_north": "none",
        "pipe_west": "none"
      },
      "apply": {
        "model": "pipe:block/fluid_pipe/side_side"
      }
    },
    {
      "when": {
        "pipe_west": "none",
        "pipe_south": "none"
      },
      "apply": {
        "model": "pipe:block/fluid_pipe/side_side",
