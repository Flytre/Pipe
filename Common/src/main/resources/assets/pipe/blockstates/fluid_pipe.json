{
  "multipart": [
    {
      "when": {
        "pipe_north": "none"
      },
      "apply": {
        "model": "pipe:block/fluid_pipe/panel",
        "x": 270
      }
    },
    {
      "when": {
        "pipe_west": "none"
      },
      "apply": {
        "model": "pipe:block/fluid_pipe/panel",
        "x": 270,
        "y": 270
      }
    },
    {
      "when": {
