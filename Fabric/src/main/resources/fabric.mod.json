{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contributors": [
    ""
  ],
  "contact": {
    "homepage": "${mod_homepage}",
    "sources": "${mod_source}",
    "issues": "${mod_issues}"
  },
  "icon": "icon.png",
  "license": "${mod_license}",
  "environment": "*",
  "entrypoints": {
    "client": [
      "net.flytre.pipe.ClientInitializer"
    ],
    "main": [
      "net.flytre.pipe.Initializer"
    ],
    "server": []
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11",
    "minecraft": "1.18.x",
    "flytre_lib": ">=${lib_version}",
    "java": ">=17"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.twitter": "${mod_twitter}",
        "modmenu.discord": "${mod_discord}",
        "modmenu.curseforge": "${mod_homepage}"
      }
    }
  }
}